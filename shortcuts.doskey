gm=git merge --no-ff $T
gma=git merge --abort $T
gmc=git merge --continue $T
gpu=git push $* $T
gpl=git pull $* $T
gplr=git pull --rebase $T
gr=git rebase $* $T
gs=git status $T
gss=git status --short $T
gst=git stash $T
