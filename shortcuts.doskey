g=git $T
ga=git add $T
gaa=git add . $T
gaaa=git add --all $T
gau=git add --update $T
gb=git branch $* $T
gbd=git branch --delete $T
gc=git commit $T
gcm=git commit --message $* $T
gcf=git commit --fixup $T
gco=git checkout $* $T
gcob=git checkout -b $* $T
gcom=git checkout master $T
gcos=git checkout staging $T
gcod=git checkout develop $T
gd=git diff $T
